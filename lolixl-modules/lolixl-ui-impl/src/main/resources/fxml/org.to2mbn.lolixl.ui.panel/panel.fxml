<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<fx:root
	styleClass="xl-panel-container"
	type="BorderPane"
	xmlns="http://javafx.com/javafx/8.0.60"
	xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<HBox
			styleClass="xl-panel-header-container"
			fx:id="headerContainer">
			<children>
				<ImageView
					styleClass="xl-panel-previous-button"
					fx:id="previousButton"
					fitHeight="45.0"
					fitWidth="45.0"
					pickOnBounds="true" />
				<ImageView
					styleClass="xl-panel-icon-view"
					fx:id="iconView"
					fitHeight="45.0"
					fitWidth="45.0"
					pickOnBounds="true" />
				<Label
					styleClass="xl-panel-title-label"
					fx:id="titleLabel" />
			</children>
		</HBox>
	</top>
	<center>
		<StackPane
			id="xl-panel-content-container"
			fx:id="panelContentContainer"
			BorderPane.alignment="CENTER" />
	</center>
</fx:root>
