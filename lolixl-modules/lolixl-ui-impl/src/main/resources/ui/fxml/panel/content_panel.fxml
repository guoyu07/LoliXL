<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<fx:root fx:id="rootContainer" prefHeight="440.0" prefWidth="850.0" type="BorderPane" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.to2mbn.lolixl.ui.impl.component.view.ContentPanelView">
   <top>
      <HBox fx:id="headerContainer" prefHeight="50.0" prefWidth="850.0" spacing="2.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="content_panel_previous_button" fx:id="previousButton" mnemonicParsing="false" prefHeight="45.0" prefWidth="45.0" />
            <ImageView fx:id="iconView" fitHeight="45.0" fitWidth="45.0" pickOnBounds="true" preserveRatio="true" />
            <Label id="content_panel_title_label" fx:id="titleLabel" prefHeight="45.0" prefWidth="750.0" />
         </children>
         <padding>
            <Insets left="2.0" top="2.0" />
         </padding>
      </HBox>
   </top>
   <center>
      <BorderPane fx:id="paneContainer" prefHeight="390.0" prefWidth="850.0" BorderPane.alignment="CENTER" />
   </center>
</fx:root>
